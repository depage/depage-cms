        <tab type="user" url="@ref admin-user-management" title="User Management" />
        <tab type="user" url="@ref admin-tasks" title="Tasks" />
        -->
    </tab>

    <tab type="examples" visible="no" title="Examples" intro=""/>

    <tab type="classes" visible="no" title="Classes">
