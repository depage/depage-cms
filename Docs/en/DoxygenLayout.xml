    <tab type="user" url="@ref get-started" title="Get Started">
        <tab type="user" url="@ref get-started" title="Introduction" />
        <tab type="user" url="@ref editing-pages" title="Editing Pages" />
        <tab type="user" url="@ref publishing" title="Publishing" />
